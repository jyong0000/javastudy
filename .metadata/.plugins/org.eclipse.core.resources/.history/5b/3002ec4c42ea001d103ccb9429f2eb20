package singletone.print;

public class Printer {
	//필드 
	private static Printer printer = null;
	
	//생성자
	private Printer() {}
	
	public static Printer getPrinter() {
		if(printer == null)
		printer = new Printer(); //print 인스턴스 생성
		return printer;
	}
	public void print(String str) {
		System.out.println(str);
	}
	
}
